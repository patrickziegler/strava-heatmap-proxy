{
  "manifest_version": 3,
  "name": "Strava Cookie Exporter",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "https://*.strava.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{02c2d5fc-d301-4fa7-a63e-753835cfe15a}"
    }
  }
}
